n.linear_model import LinearRegression\n","model = LinearRegression(fit_intercept=False, normalize=False, copy_X=False, n_jobs=None)\n","for i in range(4, 5):\n","  print(i)\n","  JH = np.append(Johansen, Y[i])\n","  for j in range(N):\n","    if i!=j:\n","      print(j)\n","      reg = model.fit(Y[j].reshape(-1, 1), Y[i])\n","      resids = Y[i] - model.predict(Y[j].reshape(-1, 1))\n","      #print(resids)\n","      autoco = np.correlate(resids[1:], resids [:-1])\n","      #print(autoco)\n","      CRDW = 2*(1-autoco)\n","      #print(CRDW)\n","      if CRDW > tao:\n","        print(\"YES\")\n","        print(j)\n","        JH = np.vstack((JH, Y[j]))\n","        #print(JH)\n","        \n","print(JH)\n","print(Y)"],"execution_count":70,"outputs":[{"output_type":"stream","text":["4\n","0\n","YES\n","0\n","1\n","YES\n","1\n","2\n","YES\n","2\n","3\n","YES\n","3\n","5\n","YES\n","5\n","6\n","YES\n","6\n","7\n","YES\n","7\n","[[ 0.          0.27106657  1.75449645  2.72967518  2.51962789  3.6119421\n","   4.19172422  5.40605327]\n"," [ 0.          0.84980015  1.94941761  2.58442543  2.83152834  2.57732566\n","   4.79715806  5.49504247]\n"," [ 0.          2.3288003   5.24078417  5.66216214  7.31229619  8.91884618\n","  11.59865968 12.69903654]\n"," [ 0.          2.25385047  4.06520468  7.2948692   9.40516725 12.0890317\n","  13.16667646 14.44198781]\n"," [ 0.          2.46868167  4.17106449  5.2986549   6.89850875  9.06280603\n","  10.36300735 11.87173957]\n"," [ 0.          0.92853475  2.49264489  3.56475479  4.73619246  7.63269694\n","   9.36838181 10.61771623]\n"," [ 0.          2.14702027  3.82699774  6.09694856  7.89080176  9.26036109\n","  11.8080746  14.35455049]\n"," [ 0.          1.19024146  3.11818018  5.47006749  6.66262557  9.27453513\n","  11.36322927 12.87844379]]\n","[[ 0.          0.84980015  1.94941761  2.58442543  2.83152834  2.57732566\n","   4.79715806  5.49504247]\n"," [ 0.          2.3288003   5.24078417  5.66216214  7.31229619  8.91884618\n","  11.59